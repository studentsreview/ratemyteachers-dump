            <script type="text/javascript">
                    window.addEvent('domready',function() {

                        $$('input[name=submit_search]').addEvent('click', function(event){
                            event.stop(); // Stops the submission of the form.
                            var keyword1 = $('search_org').get("value");
                            var keyword2 = $('search_teacher').get("value");
                            if (keyword1 != null && keyword1 != "Search for School") {
                                if (has_common_words(keyword1)){ var box = new MooDialog.Alert('Please refine your search term'); }
                                else { $('school_search').submit(); }
                            } else if (keyword2 != null && keyword2 != "Search for Teacher") {
                                $('teacher_search').submit();
                            }
                        });
                        /*
                        $('search_org').addEvent('keydown', function(event) {
                            //alert('jona');
                            if(event.key == "enter") {
                                event.stop();
                                var keyword = this.get('value');
                                if (has_common_words(keyword)){ var box = new MooDialog.Alert('Please refine your search term'); }
                                else { $('school_search').submit(); }
                            }
                        });
                        */


                        $('search_teacher').addEvent('keydown', function(event) {
                            if(event.key == "enter") {
                                event.stop();
                                var keyword = this.get('value');
                                $('teacher_search').submit();
                            }
                        });
                        function has_common_words(keyword){
                            var c_words = ['college', 'high', 'elementary', 'school'];
                            var searchIndex = c_words.indexOf(keyword.toLowerCase());
                            return searchIndex != -1;
                        }
                    });
            </script>
            <div class="clear"></div>
        	</div>
