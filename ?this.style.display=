<script type="text/javascript" src="/js/core/check_results.js"></script>
<script type="text/javascript">
    window.addEvent('domready',function() {
        if ($$('input[name=removed-search-org-submit]') != null) {
            $$('input[name=search-org-submit]').addEvent('click', function(event){
                event.stop(); // Stops the submission of the form.
                var keyword = $('search_input_expand').get("value");
                if (keyword != null && keyword != "Search Here") {
                    if (has_common_words(keyword)){ new MooDialog.Alert('Please refine your search term'); }
                    else { $('search_expand').submit(); }
                }
            });
        }

        if ($('removed_search_input_expand') != null) {
            $('search_input_expand').addEvent('keydown', function(event) {
                if(event.key == "enter") {
                    event.stop();
                    var keyword = this.get('value');
                    if (has_common_words(keyword)){ new MooDialog.Alert('Please refine your search term'); }
                    else { 

                        var data = "search_org=" + keyword + "&is_ajax=true";
                        var submit_url = {
                                "school" : 'http://www.ratemyteachers.com/search/school',
                                "teacher" : 'http://www.ratemyteachers.com/search/people'
                            };
                        var options = { 
                            'url'           : 'http://www.ratemyteachers.com/search/get_results',
                            'method'        : 'get',
                            'data'          : data,
                            'submit_url'    : submit_url,
                            'keyword'       : keyword,
                            'append_to'     : document.body
                        }
                        check_results = new Check_Results(options);
                        check_results.submit();
                    }
                }
            });
        }
        function has_common_words(keyword){
            var c_words = ['college', 'high', 'elementary', 'school'];
            var searchIndex = c_words.indexOf(keyword.toLowerCase());
            return searchIndex != -1;
        }
    });
</script>				<!--end search-->

