<script type="text/javascript">
    jQuery.noConflict();
    (function($){
        $(document).ready(function() {
            $('#cse-search-box, #cse-search-box1').submit(function(event){
                event.preventDefault();
                var self = this;
                //self.submit();
                if ($.cookie('disable_popup_search') == 'FALSE' || $.cookie('disable_popup_search') == null) {
                    FB.Event.subscribe('edge.create',
                        function(response) {
                            $('#countdown').countdown('pause');
                            $('#countdown').attr("style", "display:none");
                        }
                    );
                        
                    FB.Event.subscribe('edge.remove',
                        function(response) {
                            $('#countdown').countdown('pause');
                            $('#countdown').attr("style", "display:none");
                        }
                    );
                    
                    $('input[value="Post to Facebook"]').click(function(){
                       console.log('test');
                    });

                    $.fancybox({
                            'width': '250',
                            'height': '110',
                            'autoScale': true,
                            'transitionIn': 'fade',
                            'transitionOut': 'fade',
                            'type': 'inline',
                            'href': '#popup-for-search',
                            'afterClose' : function() {
                                $.cookie("disable_popup_search", "TRUE", { expires: 1 });
                                $.fancybox.close();
                                self.submit();
                            },
                            'afterShow' : function () {
                                $('#countdown').countdown({
                                    compact :true,
                                    layout: 'This will close in {sn} seconds.',
                                    format :'S',
                                    until: 8,
                                    onExpiry : function(){
                                        $.fancybox.close();
                                        self.submit();
                                    }
                                });
                            }
                    });
                } else {
                    self.submit();
                }
            });

            $("input[name=popup-checkbox]").change(function(){
                if ($(this).is(":checked")) {
                    $.cookie("disable_popup_search", "TRUE", { expires: 1 });
                    $.fancybox.close();
                } else {
                    $.cookie("disable_popup_search", null);
                }
            });
        });
    })(jQuery);
</script>
