<script type="text/javascript">
    window.addEvent('domready',function() {
        /*** Begin Search Expand/Collapse */
        var status = {
            'true': 'open',
            'false': 'close'
        };
        var myVerticalSlide = new Fx.Slide('vertical_slide').hide();

        if ($('search_link') != null) {
            $('search_link').addEvent('click', function(event) {
                if (status[myVerticalSlide.open] == "open") {
                    $('vertical_slide').removeClass('search');
                } else {
                    $('vertical_slide').addClass('search');
                }
                event.stop();
                myVerticalSlide.toggle();
            });
        }

        //var rate_link = $$('.rate_link');
        //rate_link.each(rate_link_add_click_event);
        //
        //function rate_link_add_click_event (el) {
        //    el.addEvent('click', function(event) {
        //        if (status[myVerticalSlide.open] == "open") {
        //            $('vertical_slide').removeClass('search');
        //        } else {
        //            $('vertical_slide').addClass('search');
        //        }
        //        event.stop();
        //        myVerticalSlide.toggle();
        //    });
        //}
        //
        //if ($('rate_link') != null) {
        //    $('rate_link').addEvent('click', function(event) {
        //        if (status[myVerticalSlide.open] == "open") {
        //            $('vertical_slide').removeClass('search');
        //        } else {
        //            $('vertical_slide').addClass('search');
        //        }
        //        event.stop();
        //        myVerticalSlide.toggle();
        //    });
        //}

        if ($('school_link') != null) {
            $('school_link').addEvent('click', function(event) {
                if (status[myVerticalSlide.open] == "open") {
                    $('vertical_slide').removeClass('search');
                } else {
                    $('vertical_slide').addClass('search');
                }
                event.stop();
                myVerticalSlide.toggle();
            });
        }

        /*
        $$('input[name=search_org]').addEvent('blur', function(event){
			//alert('jona2');
            event.stop();
            myVerticalSlide.slideOut();
        });
        */
		/*Added by jonathan 07/20/2012
		$$('input[name=search_org]').addEvent('click', function(event){
			//alert('jona3');
            event.stop();
            myVerticalSlide.slideOut();
        });
		*/

		$$('input[name=search_teacher]').addEvent('click', function(event){
			//alert('jona3');
            event.stop();
            myVerticalSlide.slideOut();
        });
		/*end*/
        myVerticalSlide.addEvent('complete', function() {
            $('vertical_status').set('text', status[myVerticalSlide.open]);
        });
        /*** End Search Expand/Collapse */

        /*** Begin Textbox Watermark */
        var values = new Array();
        var inputs = new Array();

        function addWaterMark(el){
            try {
                values.push(el.value);
                el.onfocus = function ()	{
                    if (el.value == values[inputs.indexOf(el)]) {
                        el.value = '';
                        el.className = el.className.replace("watermarkOn", "");
                    }
                }
                el.onblur = function () {
                    if(el.value === ''){
                        el.value = values[inputs.indexOf(el)];
                        el.className += " watermarkOn";
                    }
                }
            } catch(e) {dbug.log('addWaterMark: ', e)}
        };

        function addValue(el){
            try {
                //if (el.name == "search_org") el.value = "Search for School"
                if (el.name == "search_teacher") el.value = "Search for Teacher"
                if (el.id == "search_input_expand") el.value = "Search Here"
            } catch(e) {dbug.log('addWaterMark: ', e)}
        };

        inputs = $$('input[name^=search],input[name=search_input_expand],input[name^=newsletter],input[class^=rating_fields_input]');
        inputs.each(addValue);
        inputs.each(addWaterMark);
        /*** End Textbox Watermark */
    });
</script>
