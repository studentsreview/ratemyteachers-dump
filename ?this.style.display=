                    if ($("select#country-select").val() != "" ) {
                        switch ($("select#country-select").val())
                            {
                                case "United States":
                                    //window.location = 'http://us.richard.ratemyteachers.com/';
                                    $("input[name=cr]").val('countryUS');
                                    break;
                                case "Canada":
                                    //window.location = 'http://ca.richard.ratemyteachers.com/';
                                    $("input[name=cr]").val('countryCA');
                                    break;
                                case "United Kingdom":
                                    //window.location = 'http://uk.richard.ratemyteachers.com/';
                                    $("input[name=cr]").val('countryUK');
                                    break;
                                case "Australia":
                                    //window.location = 'http://au.richard.ratemyteachers.com/';
                                    $("input[name=cr]").val('countryAU');
                                    break;
                                case "Ireland":
                                    //window.location = 'http://ie.richard.ratemyteachers.com/';
                                    $("input[name=cr]").val('countryIE');
                                    break;
                                case "New Zealand":
                                    //window.location = 'http://nz.richard.ratemyteachers.com/';
                                    $("input[name=cr]").val('countryNZ');
                                    break;
                                default:
                                   //window.location = 'http://us.richard.ratemyteachers.com/';
                                   $("input[name=cr]").val('countryUS');
                            }
                    }

                });

	})
          })(jQuery);
